<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sunkai.activityproxyhost">

    <application
        android:allowBackup="true"
        android:label="asd"
        >
        <activity android:name=".myapplicaiton.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".myapplicaiton.PlugProxyActivity"/>
        <activity android:name=".myapplicaiton.ProxyActivity"/>
        <activity android:name=".myapplicaiton.BaseActivity"/>
    </application>

</manifest>