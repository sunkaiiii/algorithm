{
    "tasks": [
        {
            "type": "shell",
            "label": "gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "-I",
                "${workspaceFolder}/lib",
                "${file}",
                "${workspaceFolder}/libtlpi.a",
                "${workspaceFolder}/libtlpi.a",
                "${workspaceFolder}/signals/signal_functions.a",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "/usr/bin"
            }
        }
    ],
    "version": "2.0.0"
}